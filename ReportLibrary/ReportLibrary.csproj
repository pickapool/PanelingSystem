<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Update="GradeSheetDataset.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>GradeSheetDataset.xsd</DependentUpon>
    </Compile>
    <Compile Update="masterlistdataset.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>masterlistdataset.xsd</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="GradeSheetDataset.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>GradeSheetDataset.Designer.cs</LastGenOutput>
    </None>
    <None Update="masterlistdataset.xsd">
      <Generator>MSDataSetGenerator</Generator>
      <LastGenOutput>masterlistdataset.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
